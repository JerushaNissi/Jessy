function nthPrime(n) 
{
output = true;  
    for (i=2 ; i<num ; i++) 
    {
        if (n%i === 0)  
        {
           output = false ; 
           break;
        }
    }
return output
}

function PrimeNumber(n) 
{
	var count = 0;
    for (i=2 ; i<10000 ; i++)  
    {
        if (nthPrime(i) === true) 
        {
            count = count + 1; 
        }
        if (count === n) 
        {
            return i;
            break;
        } 
    }
}